{
  "name": "MQTT Discord Bot",
  "version": "1.0.17",
  "slug": "mqtt_discord_bot",
  "init": false,
  "description": "Who is currently online on Discord and with whom.",
  "url": "https://github.com/ZackEndboss/hassio-addons",
  "startup": "application",
  "boot": "manual",
  "arch": ["armhf", "armv7", "aarch64", "amd64", "i386"],
  "ports": {},
  "privileged": ["NET_ADMIN"],
  "ingress": true,
  "map": ["config:rw"],
  "codenotary": "discord2mqtt+hassio@zacky.org",
  "schema": {
    "mqtt_broker": "str",
    "mqtt_port": "int",
    "mqtt_username": "str",
    "mqtt_password": "str",
    "discord_token": "str",
    "discord_guild_id": "str"
  },
  "options": {
    "mqtt_broker": "localhost",
    "mqtt_port": 1883,
    "mqtt_username": "user",
    "mqtt_password": "password",
    "discord_token": "your_discord_token",
    "discord_guild_id": "your_server_id"
  }
}